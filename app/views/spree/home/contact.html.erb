<h1>Contact Us</h1>
<div class="row">
  <div class="col-md-6">
    <h1>Form</h1>
  </div>
  <div class="col-md-6">
    <div id="map" style="height: 400px;width: 100%;"></div>
    <script>
        function initMap() {
            var uluru = {lat: -25.363, lng: 131.044};
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 4,
                center: uluru
            });
            var marker = new google.maps.Marker({
                position: uluru,
                map: map
            });
        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC5Kg6RI10k2-cxdCjqLBhCxzkDwbZtUAQ&callback=initMap">
    </script>
  </div>
</div>